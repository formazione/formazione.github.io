   "source": [
    "x = \"10:18\"\n",
    "\n",
    "def dec(f):\n",
    "    def wrapper(t):\n",
    "        print(f(t))\n",
    "    return wrapper\n",
    "\n",
    "\n",
    "@dec \n",
    "def convert(timestr):\n",
    "    h,m = timestr.split(\":\")\n",
    "    n = int(h)*60 + int(m)\n",
    "    return n\n",
    "\n",
    "xtm = convert(\"10:18\")"
   ]
